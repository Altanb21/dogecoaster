        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "prettier": "^2.6.2",
        "prettier-plugin-solidity": "^1.0.0-beta.19",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.6.4"
